{
  "Consul": {
    "IP": "localhost",
    "Port": "5726",
    "ServiceName": "demo-service",
    "Weight": "1",
    "Client": {
      "Address": "http://localhost:8500/",
      "Datacenter": "dc1"
    },
    "HeartCheck": {
      "Interval": "12",
      "HTTP": "/healthcheck",//避免与已有的Controllers重名
      "Timeout": "5",
      "DeregisterCriticalServiceAfter": "60"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*"
}
